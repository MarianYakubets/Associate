<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns="http://www.w3.org/ns/widgets"
        xmlns:gap="http://phonegap.com/ns/1.0"
        id="com.phonegap.example"
        versionCode="1"
        version="1.0.0">

    <plugin name="cordova-plugin-firebase" spec="0.1.18" />

    <name>Color Faces</name>
    <content src="index.html"/>

    <access origin="*" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <icon src="icons/icon.png" density="ldpi" />
        <icon src="icons/icon.png" density="mdpi" />
        <icon src="icons/icon.png" density="hdpi" />
        <icon src="icons/icon.png" density="xhdpi" />
        <allow-intent href="market:*" />
    </platform>

    <description>
        An example for phonegap build docs.
    </description>

    <author href="https://build.phonegap.com" email="no@no.com">
        Mifik
    </author>

</widget>